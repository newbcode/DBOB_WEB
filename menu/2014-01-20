			<script type="text/javascript">	urlConfig = {		http: 'http://www.welstory.com',		https: 'https://www.welstory.com:3443'	}</script><script language="javascript" src="/js/menu/top.js"></script>
<script type="text/javascript">
	function swfDiary(){
		if (true) {
			window.open("http://www.welstory.com/mywelstory/diary/index.jsp","mydiary","width=800,height=600, toolbar=no,scrollbars=auto,resizable=yes,copyhistory=no");
		} else {
			alert("회원 전용 서비스입니다.");
			location.href	= "/common/login.jsp";
		}
	}
	String.prototype.trim = function() {
	  return this.replace(/(^\s*)|(\s*$)/g, "");
	 }
	function check_text(){

		var str = document.frm_search.search_word.value;
		//document.frm.search_word.value = Trim(str);
		document.frm_search.search_word.value = str.trim();
		if(document.frm_search.search_word.value  == ""){
			alert("검색어를 입력해주세요");
			document.frm_search.search_word.focus();
			return;
		}else{
			document.frm_search.action = "http://www.welstory.com/helpdesk/totalsearch/totalSearch.jsp?mMode=4";
			document.frm_search.submit();
		}
	}

	function CheckEnt(urlname){
		if (event.keyCode == 13) {
			if(!check_text()){
				return;
			}
		}else{
			return;
		}
	}
</script>
